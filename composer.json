{
    "name": "mcfedr/memcached-bundle",
    "type": "symfony-bundle",
    "description": "The smallest, simplest memcached bundle",
    "keywords": ["memcached", "memcache", "symfony", "session"],
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "^2.8|^3.0",
        "ext-memcached": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": { "Mcfedr\\MemcachedBundle\\": "src/Mcfedr/MemcachedBundle" }
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\MemcachedBundle\\": "tests/Mcfedr/MemcachedBundle" }
    }
}
